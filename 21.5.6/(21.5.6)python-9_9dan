'''
while True:
    dan = int(input('몇 단 출력 : '))
    
    if dan == 0:
        print('0 단은 모두 0입니다.')
        break 

    if dan < 2 or dan > 9:
        print('2부터 9까지의 숫자를 입력해주세요.')
        continue
    
    for i in range(1, 10):
        print(' {} * {} = {} '.format(dan, i, dan * i))
