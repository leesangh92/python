'''
while True:
    
    score = int(input(' 점수 입력 : '))

    if score > 100 or score <0:
        print('점수를 정확히 입력해주세요')
        continue
        
    if score >= 90:
        print( score, '수')
    elif score >= 80:
        print( score, '우')
    elif score >= 70:
        print( score, '미')
    elif score >= 60:
        print( score, '양')
    elif score >= 50:
        print( score, '가')
    else:
        print( score, '분발하세요') # 다중 if 문
